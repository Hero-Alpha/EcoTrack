<% layout("/layouts/boilerplate-user") %>
    <link rel="stylesheet" href="/css/partners.css">
    <main class="container">
        <section class="locations-section">
            <h2 class="section-title">Our Locations</h2>
            <div class="locations-grid">

                <div class="location-card">
                    <div class="location-header">
                        <i class="fas fa-map-marker-alt"></i>
                        <h3>Delhi</h3>
                    </div>
                    <div class="location-body">
                        <p>Nawada, Uttam Nagar</p>
                        <div class="coordinates">
                            <span>Lat: 40.7128</span>
                            <span>Lng: -74.0060</span>
                        </div>
                    </div>
                    <div class="location-partners">
                        <h4>Logistics Partners:</h4>
                        <div class="partners-list">

                            <div class="partner-card">
                                <img src="/images/partner1.jpg" alt="Green Logistics logo">
                                <div class="partner-info">
                                    <h5>Green Logistics</h5>
                                    <div class="partner-services">
                                        <span class="service-tag">Road</span>
                                        <span class="service-tag">Air</span>
                                    </div>
                                    <div class="partner-rating">
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star-half-alt"></i>
                                        <span>(4.5)</span>
                                    </div>
                                </div>
                            </div>
                            <!-- Partner 2 -->
                            <div class="partner-card">
                                <img src="/images/partner2.jpg" alt="EcoShip logo">
                                <div class="partner-info">
                                    <h5>EcoShip</h5>
                                    <div class="partner-services">
                                        <span class="service-tag">Air</span>
                                        <span class="service-tag">Rail</span>
                                    </div>
                                    <div class="partner-rating">
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="far fa-star"></i>
                                        <span>(4.2)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="location-card">
                    <div class="location-header">
                        <i class="fas fa-map-marker-alt"></i>
                        <h3>Hari Nagar</h3>
                    </div>
                    <div class="location-body">
                        <p>People Chowk</p>
                        <div class="coordinates">
                            <span>Lat: 37.7749</span>
                            <span>Lng: -122.4194</span>
                        </div>
                    </div>
                    <div class="location-partners">
                        <h4>Logistics Partners:</h4>
                        <div class="partners-list">
                            <!-- Partner 1 -->
                            <div class="partner-card">
                                <img src="/images/partner3.jpg" alt="Bay Area Logistics logo">
                                <div class="partner-info">
                                    <h5>Bay Area Logistics</h5>
                                    <div class="partner-services">
                                        <span class="service-tag">Road</span>
                                        <span class="service-tag">Sea</span>
                                    </div>
                                    <div class="partner-rating">
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="far fa-star"></i>
                                        <span>(4.3)</span>
                                    </div>
                                </div>
                            </div>
                            <!-- Partner 2 -->
                            <div class="partner-card">
                                <img src="/images/partner2.jpg" alt="Pacific Transport logo">
                                <div class="partner-info">
                                    <h5>Pacific Transport</h5>
                                    <div class="partner-services">
                                        <span class="service-tag">Air</span>
                                        <span class="service-tag">Rail</span>
                                    </div>
                                    <div class="partner-rating">
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star"></i>
                                        <i class="fas fa-star-half-alt"></i>
                                        <span>(4.6)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>